  "jest": {
    "preset": "vdkweb-jest-preset",
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^.+\\.(png|jpg|gif|css|scss)$": "<rootDir>/helpers/fileMock.js",
      "^~/(.*)": "<rootDir>/$1",
      "^#/(.*)": "<rootDir>/src/$1"
    },
    "resetMocks": true,
    "coveragePathIgnorePatterns": [
      "__tests__",
      "src/unused",
      "src/static"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/*.stories.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/helpers/testSetup.js",
    "notify": true
  }
