  "license": "UNLICENSED",
  "private": true,
  "repository": "bitbucket:accedo/halil-myapp",
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client && npm run build:pkg-json",
    "build-storybook": "build-storybook",
    "build:client": "cross-env NODE_ENV=production && webpack --config=./webpack/prod/prod-client-config.js",
    "build:dev": "webpack --config=./webpack/dev/dev-config.js",
    "build:dll": "webpack --config=./webpack/webpack.dll.js",
    "build:pkg-json": "cross-env NODE_ENV=production && node ./helpers/createProdPackageJson",
    "build:server": "cross-env NODE_ENV=production && webpack --config=./webpack/prod/prod-server-config.js",
    "build:static": "cross-env NODE_ENV=production && npm run clean && webpack --config=./webpack/prod/prod-static-config.js",
    "build:tv": "npm run clean && cross-env DEV_ENV=tv NODE_ENV=production webpack --config=./webpack/prod/prod-tv-config.js",
    "clean": "rimraf ./build",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"./src/**/*.scss\" \"./src/**/*.css\" --syntax scss",
    "lint:css:fix": "stylefmt --config .stylelintrc -r \"./src/**/*.scss\" \"./src/**/*.css\"",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "package": "npm run build && npm run tar",
    "package:static": "npm run build:static && npm run tar:static",
    "postinstall": "npm run build:dll",
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm run test:once",
    "prettier": "prettier --write \"{src,helpers,webpack}/**/*.*css\" \"{src,helpers,webpack,generator}/!(dll)/**/*.js*\"",
    "sonar": "npm run test:once && node ./helpers/sonar.js",
    "start": "npm run dev",
    "start-dev": "cross-env NODE_ENV=development && node ./helpers/startDevServer",
    "storybook": "start-storybook -p 6006",
    "tar": "cd build && mkdir -p ../dist && tar -cjf ../dist/`echo ${npm_package_name}`-`echo ${npm_package_version}`.tar.bz2 . && cd ..",
    "tar:static": "cd build && mkdir -p ../dist && tar -cjf ../dist/`echo ${npm_package_name}`-static-`echo ${npm_package_version}`.tar.bz2 --exclude=./assets.json --exclude=server* . && cd ../..",
    "test": "jest --watch",
    "test:once": "jest",
    "view:coverage": "opn coverage/lcov-report/index.html",
    "watch-client": "cross-env NODE_ENV=development && node ./webpack/dev/dev-server-entry"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --tab-width 4 --write",
      "git add"
    ]
  },
